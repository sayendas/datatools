          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FilmLengthDesc">
          <Value>=Switch(Fields!FilmRunTimeMinutes.Value&lt;100,"Short",
        Fields!FilmRunTimeMinutes.Value&lt;150,"Medium",
		Fields!FilmRunTimeMinutes.Value&lt;200,"Long",
		TRUE,"Epic")</Value>
        </Field>
        <Field Name="FilmLengthSentence">
          <Value>=Fields!FilmName.Value &amp; " is a " &amp; Fields!FilmLengthDesc.Value &amp; " film "</Value>
