  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT tblFilm.FilmRunTimeMinutes, tblCertificate.Certificate, tblStudio.StudioName
FROM     tblFilm INNER JOIN
                  tblCertificate ON tblFilm.FilmCertificateID = tblCertificate.CertificateID INNER JOIN
                  tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID
ORDER BY tblStudio.StudioName</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmRunTimeMinutes">
          <DataField>FilmRunTimeMinutes</DataField>
