            <FilterValue>=Parameters!EndDate.Value</FilterValue>
          </FilterValues>
        </Filter>
        <Filter>
          <FilterExpression>=Fields!FilmName.Value</FilterExpression>
          <Operator>Like</Operator>
          <FilterValues>
            <FilterValue>="*" &amp; Parameters!FilmName.Value &amp; "*"</FilterValue>
          </FilterValues>
        </Filter>
        <Filter>
          <FilterExpression>=Fields!FilmOscarWins.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!Oscarwins.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
