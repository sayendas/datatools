    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT tblFilm.FilmName, tblFilm.FilmOscarWins, tblCountry.CountryName
FROM     tblCountry INNER JOIN
                  tblFilm ON tblCountry.CountryID = tblFilm.FilmCountryID
ORDER BY tblFilm.FilmName</CommandText>
      </Query>
